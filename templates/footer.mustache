{{!
    Page footer.
}}
{{! not displayed: goto-top-link }}
<footer id="page-footer" class="">
    <div class="container">
        <div id="course-footer">{{{ output.course_footer }}}</div>

        <div class="footer-info">
        {{#footercontent }}
            <div class="footer-items">
                {{{ footercontent }}}
            </div>
        {{/ footercontent }}
        </div>

        {{#footerlinkscontent }}
            <ul class="footer-links m-0">
                {{{ footerlinkscontent }}}
            </ul>
        {{/footerlinkscontent }}

        {{! not displayed: output.page_doc_link }}

        {{! not displayed: output.login_info  }}
        {{! not displayed:  output.home_link }}
        <div class="footer-standard">
            {{{ output.standard_footer_html }}}
        </div>
        {{{ output.standard_end_of_body_html }}}
    </div>
</footer>
